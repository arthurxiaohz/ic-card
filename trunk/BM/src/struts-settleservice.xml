			class="cn.net.iccard.bm.settleservice.action.struts.TblStlCleaningDetailAction" method="saveTblStlCleaningDetail">
			<result name="success" type="redirect-action">tblStlCleaningDetailList</result>
			<interceptor-ref name="modelParamsStack" />
		</action>
					
