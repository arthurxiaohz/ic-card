<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<environment dir="web/WEB-INF/metadata">
    <database>
        <dbtype>MYSQL</dbtype>
        <connection>
            <username>root</username>
            <password>mysql</password>
            <url>jdbc:mysql://localhost:3306/demo?useUnicode=true&amp;characterEncoding=UTF-8</url>
            <driverClass>com.mysql.jdbc.Driver</driverClass>
        </connection>
    </database>
    <javaEnvironment>
        <homePath></homePath>
        <libraryDir>E:/platform/generater/lib</libraryDir>
    </javaEnvironment>
    <generate>
        <appName>hi</appName>
        <orgPackage>org.hi</orgPackage>
        <templetDir>templates</templetDir>
        <srcOutput>src</srcOutput>
        <standardOutput>web</standardOutput>
        <ormType>ibatis</ormType>
        <viewType>struts</viewType>
        <pageFrameType>dwz</pageFrameType>
    </generate>
</environment>
